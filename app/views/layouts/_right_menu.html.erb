<div class="container">
  <div class="row">
    <div class="offset-1 col-11">
      <div class="diary-table new-diary-list">
        <h2 class="col-12" style="text-align:center;">新着日記</h2>
        <% @diary_new.first(9).each do |diary| %>
          <div class="new-diary">
            <%= link_to diary_path(diary.id) do %>
              <%= attachment_image_tag diary, :diary_image, :fill, 1000, 1000, fallback: "no_image_square.jpg", size: "80x80", class:"new-diary-img" %>
            <% end %>
          </div>
        <% end %>
      </div>

      <div class="tag-menu mt-5">
        <h2 class="col-12" style="text-align:center;">人気のタグ</h2>

      </div>
    </div>
  </div>
</div>